<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<SplitPane dividerPositions="0.414" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apptSys.controller.CustListViewController">
   <items>
      <VBox>
         <children>
            <Region VBox.vgrow="ALWAYS" />
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <HBox VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="100.0" text="Customer Name" textAlignment="CENTER">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="custNameTxt" alignment="CENTER" prefHeight="25.0" prefWidth="250.0">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="70.0" text="Phone #">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="custPhoneTxt" prefHeight="25.0" prefWidth="200.0">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <Region HBox.hgrow="ALWAYS" />
               </children></HBox>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <HBox VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="70.0" text="Address 1" textAlignment="CENTER">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="custAdd1Txt" prefHeight="25.0" prefWidth="250.0">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="70.0" text="Address 2" textAlignment="CENTER">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="custAdd2Txt" prefHeight="25.0" prefWidth="250.0">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                        </HBox>
                     </children></VBox>
                  <Region HBox.hgrow="ALWAYS" />
               </children></HBox>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <HBox VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="40.0" text="City" textAlignment="CENTER">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <TextField fx:id="custCityTxt" prefHeight="25.0" prefWidth="125.0">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </TextField>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="50.0" text="Country">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin>
                              </Label>
                              <ChoiceBox fx:id="custCountryOpt" prefWidth="125.0">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </ChoiceBox>
                           </children>
                        </HBox>
                        <HBox VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="100.0" text="Postal Code">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                              <TextField fx:id="custPCTxt" prefHeight="25.0" prefWidth="150.0">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin></TextField>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <Region HBox.hgrow="ALWAYS" />
               </children></HBox>
            <HBox VBox.vgrow="ALWAYS">
               <children>
                  <Button mnemonicParsing="false" onAction="#onNewBtn" prefHeight="25.0" prefWidth="60.0" text="New">
                     <HBox.margin>
                        <Insets left="50.0" right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onDelBtn" prefHeight="25.0" prefWidth="60.0" text="Delete">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Button>
                  <Region HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#onSaveBtn" prefHeight="25.0" prefWidth="60.0" text="Save">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onCnxBtn" prefHeight="25.0" prefWidth="60.0" text="Cancel">
                     <HBox.margin>
                        <Insets left="10.0" right="50.0" />
                     </HBox.margin>
                  </Button>
               </children></HBox>
         </children>
      </VBox>
      <TableView fx:id="custListTbl">
        <columns>
          <TableColumn fx:id="custNameCol" prefWidth="175.0" text="Name" />
          <TableColumn fx:id="custPhoneCol" minWidth="0.0" prefWidth="175.0" text="Phone #" />
            <TableColumn fx:id="custCityCol" prefWidth="175.0" text="City" />
            <TableColumn fx:id="custCountryCol" minWidth="0.0" prefWidth="175.0" text="Country" />
        </columns>
      </TableView>
   </items>
</SplitPane>
