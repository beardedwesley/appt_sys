            <TableView fx:id="day5Tbl" GridPane.columnIndex="4">
              <columns>
                <TableColumn fx:id="day5Col" prefWidth="170.0" text="TodayPlus4" />
              </columns>
            </TableView>
            <TableView fx:id="day6Tbl" GridPane.columnIndex="5">
