          <ColumnConstraints fx:id="satCol" hgrow="SOMETIMES" minWidth="25.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <children>
            <TableView fx:id="day1Tbl" GridPane.columnIndex="0">
              <columns>
                <TableColumn fx:id="day1Col" prefWidth="170.0" text="Today" />
