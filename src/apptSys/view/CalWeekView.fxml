<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane hbarPolicy="NEVER" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apptSys.controller.CalWeekViewController">
   <content>
      <GridPane gridLinesVisible="true" hgap="1.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="1200.0">
        <columnConstraints>
          <ColumnConstraints fx:id="sunCol" hgrow="SOMETIMES" minWidth="25.0" prefWidth="100.0" />
          <ColumnConstraints fx:id="monCol" hgrow="SOMETIMES" minWidth="25.0" prefWidth="100.0" />
